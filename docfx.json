{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Runtime/**.cs"
          ]
        }
      ],
      "dest": "_TEMP_DOCS/API",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "_TEMP_DOCS/API/**.yml",
          "_TEMP_DOCS/API/index.md"
        ]
      },
      {
        "files": [
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_TEMP_DOCS/output",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}